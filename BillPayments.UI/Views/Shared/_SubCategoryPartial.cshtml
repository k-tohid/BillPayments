@model ReadCategoryDTO

<li>
    <a href="#" class="category-item" data-id="@Model.Id">@Model.Title</a>

    @if (Model.SubCategories != null && Model.SubCategories.Any())
    {
        <ul>
            @foreach (var subCategory in Model.SubCategories)
            {
                @await Html.PartialAsync("_SubCategoryPartial", subCategory)
            }
        </ul>
    }
</li>
